{
  "env": {
      "browser": true,
      "es2021": true,
      "jest": true
  },
  "extends": [
      "standard"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "overrides": [
    {
      "files": [
        "**/*.spec.ts",
        "**/*.test.ts"
      ]
    }
  ],
  "plugins": [
      "@typescript-eslint"
  ],
  "rules": {}
}
